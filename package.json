{
  "name": "server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "rmCache": "rimraf ./dist",
    "dev": "nodemon",
    "dev:": "nodemon",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start:test": "npm run build:ts && cross-env IS_TEST=1 node ./server.js ",
    "start": "npm run build:ts && cross-env node ./server.js ",
    "build:ts": "rimraf ./dist && tsc",
    "watch:ts": "tsc -w",
    "dev:cli": "npm run build:ts && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js",
    "ts:start": "ts-node ./dev.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/cors": "^8.2.1",
    "@fastify/jwt": "^6.7.1",
    "@fastify/multipart": "^7.6.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/static": "^6.10.1",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@prisma/client": "^4.14.1",
    "@sinclair/typebox": "^0.25.24",
    "close-with-grace": "^1.2.0",
    "dotenv": "^16.0.3",
    "fastify": "^4.17.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.5.0",
    "js-md5": "^0.7.3",
    "moment": "^2.29.4",
    "prisma": "^4.14.1",
    "rimraf": "^4.4.1"
  },
  "devDependencies": {
    "@types/js-md5": "^0.7.0",
    "@types/node": "^18.16.12",
    "@types/tap": "^15.0.8",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "fastify-tsconfig": "^1.0.1",
    "nodemon": "^2.0.22",
    "tap": "^16.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}